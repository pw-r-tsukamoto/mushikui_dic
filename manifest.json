{
    "manifest_version": 3,
    "name": "翻訳v2",
    "version": "1.1.1",
    "description": "ほげほげ。ふがふが。いい名前あったら名前変更",
    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "js": [ "src/main.js" ],
            "css": [ "src/main.css" ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["config.json"],
            "matches": [ "<all_urls>" ]
        }
    ],
    "action": {
        "default_title": "設定",
        "default_popup": "src/browser_action/browser.html"
    },
    "permissions": [
        "storage"
    ]
}